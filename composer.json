{
    "name": "binafy/laravel-user-monitoring",
    "description": "Monitor your user and all activity on your application",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "miladev-ent",
            "email": "milwad.dev@gmail.com",
            "role": "author"
        }
    ],
    "autoload": {
        "psr-4": {
            "Binafy\\LaravelUserMonitoring\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0",
        "jenssegers/agent": "^2.6.4"
    },
    "require-dev": {
        "pestphp/pest-plugin-laravel": "^2.1.0",
        "orchestra/testbench": "^8.5.10"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
